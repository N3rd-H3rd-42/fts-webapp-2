{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"]
    // "prettier/prettier": "error"
  }
}
