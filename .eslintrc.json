{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "prettier", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "plugings": ["prettier", "eslint-config-prettier"],
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-console": "off",
    "prettier/prettier": ["error"]
  }
}
